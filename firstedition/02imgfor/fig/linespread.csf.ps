%% pattern stuff
    /BPSIDE 32 def	%% pixels per pattern side
    /PATFREQ 3.0 def	%% pattern pixels per mm
    /dp_mat [PATFREQ 0 0 PATFREQ 0 0] def
    /dp_pw BPSIDE def	%% pattern pixel width
    /dp_ph BPSIDE def	%% pattern pixel height
