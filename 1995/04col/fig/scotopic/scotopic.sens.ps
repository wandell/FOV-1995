%
%	Definitions 
%
/inch {72 mul} def

%	PS_define_box
/box { % stack:  grey x y
newpath
moveto
-0.040000 inch -0.040000 inch rmoveto 
0.000000 inch 0.080000 inch rlineto 
0.080000 inch 0.000000 inch rlineto 
0.000000 inch -0.080000 inch rlineto 
-0.080000 inch 0.000000 inch rlineto 
0.040000 inch 0.040000 inch rmoveto 
closepath  0 setgray fill 
 } def

%	PS_define_diamond
/diamond { % stack:  grey x y
newpath
moveto
0.000000 inch -0.050000 inch rmoveto 
0.050000 inch 0.050000 inch rlineto 
-0.050000 inch 0.050000 inch rlineto 
-0.050000 inch -0.050000 inch rlineto 
0.050000 inch -0.050000 inch rlineto 
0.000000 inch 0.050000 inch rmoveto
closepath 0 setgray fill
} def

%	PS_define_cross
/cross { %  stack:  x y
newpath
moveto
0.000000 inch -0.045000 inch rmoveto
0.000000 inch 0.090000 inch rlineto 
-0.045000 inch -0.045000 inch rmoveto 
0.090000 inch 0.000000 inch rlineto 
0.090000 inch 0.000000 inch rmoveto
stroke
} def

% PS_define_circle

/circle {	%stack: xloc yloc
newpath
0.035000 inch 0 360 arc stroke 
} def

% PS_define_unit_circle

/unit_circle {	%stack:
newpath
gsave
0.0 x_scalepos 0.0 y_scalepos
1.0 x_scalepos 0.0 x_scalepos sub
1.0 1.333294 scale
0 360 arc stroke 
grestore
} def

% PS_define_variable_circle

/variable_circle {	%stack: x y radius
newpath
gsave
x_scalepos 0.0 x_scalepos sub
1.0 1.333294 scale
0 360 arc stroke 
grestore
} def

/c_label {  %stack: string x y (centers label) 
0 setgray fill
moveto dup stringwidth
-2.0 div exch -2.0 div exch
rmoveto show
} def 

/v_label {  %stack: string x y (vertical label)
0 setgray fill
moveto
90.0 rotate
dup stringwidth % copy string, determine x and y width
-2.0 div exch -2.0 div exch	%center
rmoveto show
-90.0 rotate
} def

/label {  % stack: string x y 
0 setgray fill
moveto show
} def 

/centerdash { 
 	/pattern exch def
	/pathlen pathlength cvi def
	/patternlength 0 def

	pattern 
	  { patternlength add /patternlength exch def
	  } forall

	pattern length 2 mod 0 ne
	  { /patternlength patternlength 2 mul def } if 

	/first pattern 0 get def
	/last patternlength first sub def

	/n pathlen last sub patternlength idiv def
	/endpart pathlen patternlength n mul sub
 	  last sub 2 div def
	/offset first endpart sub def

	pattern offset setdash
 } def

 /pathlength { 
  	flattenpath
	/dist 0 def

	{ /yfirst exch def /xfirst exch def
	  /ymoveto yfirst def /xmoveto xfirst def}
	{ /ynext exch def /xnext exch def
	  /dist dist ynext yfirst sub dup mul
	    xnext xfirst sub dup mul add sqrt add def
	  /yfirst ynext def /xfirst xnext def}
	{}

	{ /ynext ymoveto def /xnext xmoveto def
	  /def dist ynext yfirst sub dup mul
	    xnext xfirst sub dup mul add sqrt add def
	  /yfirst ynext def /xfirst xnext def}
	pathforall
	dist
 } def

