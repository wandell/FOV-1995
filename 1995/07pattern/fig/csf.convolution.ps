	/hx E dp_w div floor cvi def
	/ly E dp_h div floor cvi def
	/lx E dp_w div floor cvi def
	lx 1 hx	{
	    dp_w mul
	    ly 1 hy {
		dp_h mul
		E dup 3 1 roll E
		patmat currentmatrix pop
		TR
		dp_pw dp_ph inv
		dp_mat dp_proc imagemask
		patmat setmatrix
	    } for
	    pop
	} for
} bdef
